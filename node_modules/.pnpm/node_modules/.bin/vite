#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/vite@3.0.4/node_modules/vite/bin/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/vite@3.0.4/node_modules/vite/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/vite@3.0.4/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules:/Users/yamin/projet_dev/node_modules:/Users/yamin/node_modules:/Users/node_modules:/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/node_modules/vite/bin/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/node_modules/vite/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/vite@3.0.4/node_modules/vite/bin/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/vite@3.0.4/node_modules/vite/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/vite@3.0.4/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules:/Users/yamin/projet_dev/node_modules:/Users/yamin/node_modules:/Users/node_modules:/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/node_modules/vite/bin/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/node_modules/vite/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
