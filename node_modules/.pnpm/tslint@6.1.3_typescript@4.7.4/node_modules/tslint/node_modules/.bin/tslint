#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/tslint@6.1.3_typescript@4.7.4/node_modules/tslint/bin/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/tslint@6.1.3_typescript@4.7.4/node_modules/tslint/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/tslint@6.1.3_typescript@4.7.4/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules:/Users/yamin/projet_dev/node_modules:/Users/yamin/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/tslint@6.1.3_typescript@4.7.4/node_modules/tslint/bin/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/tslint@6.1.3_typescript@4.7.4/node_modules/tslint/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/tslint@6.1.3_typescript@4.7.4/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules:/Users/yamin/projet_dev/node_modules:/Users/yamin/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/tslint" "$@"
else
  exec node  "$basedir/../../bin/tslint" "$@"
fi
