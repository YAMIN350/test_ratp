#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/vitest@0.21.0/node_modules/vitest/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/vitest@0.21.0/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules:/Users/yamin/projet_dev/node_modules:/Users/yamin/node_modules:/Users/node_modules:/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/vitest/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/vitest@0.21.0/node_modules/vitest/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/vitest@0.21.0/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/.pnpm/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules:/Users/yamin/projet_dev/node_modules:/Users/yamin/node_modules:/Users/node_modules:/node_modules:/Users/yamin/projet_dev/test_ratp/node_modules/vitest/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
